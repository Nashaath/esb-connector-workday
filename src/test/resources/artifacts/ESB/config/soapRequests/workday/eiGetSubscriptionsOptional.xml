    <workdayGetSubscriptionsRecords>
        <bsvc:Request_References xmlns:bsvc="urn:com.workday/bsvc">
            <bsvc:Subscription_Reference bsvc:Descriptor="%s(descriptor)">
                <bsvc:ID bsvc:type="%s(wIDType)">%s(wID)</bsvc:ID>
