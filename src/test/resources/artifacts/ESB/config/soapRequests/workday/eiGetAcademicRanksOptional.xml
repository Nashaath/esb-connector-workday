<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/">
<soapenv:Header/>
<soapenv:Body>
    <workdayMethod>getAcademicRanks</workdayMethod>
    <workdayUserName>%s(username)</workdayUserName>
    <workdayPassword>%s(password)</workdayPassword>
    <workdayApiVersion>%s(apiVersion)</workdayApiVersion>
    <workdayApiUrl>%s(apiUrl)</workdayApiUrl>
    <workdayTenantId>%s(tenantId)</workdayTenantId>
    <workdayGetAcademicRanksRecords xmlns:bsvc="urn:com.workday/bsvc">
        <bsvc:Request_References>
            <bsvc:Academic_Rank_Reference bsvc:Descriptor="%s(descriptor)">
                <bsvc:ID bsvc:type="%s(academicRankIdType)">%s(academicRankId)</bsvc:ID>
            </bsvc:Academic_Rank_Reference>
        </bsvc:Request_References>
    </workdayGetAcademicRanksRecords>
</soapenv:Body>
</soapenv:Envelope>